{
  "name": "webhook-functions",
  "private": true,
  "main": "./dist/index.js",
  "scripts": {
    "dev": "npm run test:build && npm run serve",
    "clean": "rimraf dist coverage",
    "typecheck": "tsc --noEmit",
    "lint": "npx eslint '*/**/*.{js,jsx,ts,tsx}' --quiet --fix",
    "test:lint": "eslint '*/**/*.{js,jsx,ts,tsx}' --quiet --fix",
    "test:deploy": "npm-run-all clean test:lint typecheck",
    "test:build": "npm-run-all clean lint typecheck",
    "build": "npx swc src -d dist",
    "build:deploy": "swc src -d dist",
    "build:ts-watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "16"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "chalk": "~4.1.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^6.7.0",
    "express-useragent": "^1.0.15",
    "expresso-core": "^0.3.1",
    "firebase-admin": "^11.5.0",
    "firebase-functions": "^4.2.1",
    "helmet": "^6.0.1",
    "hpp": "^0.2.3",
    "i18next": "^22.4.12",
    "i18next-fs-backend": "^2.1.1",
    "i18next-http-middleware": "^3.3.0",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "npm-run-all": "^4.1.5",
    "qs": "^6.11.1",
    "request-ip": "^3.3.0",
    "slugify": "^1.6.5",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.41",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-useragent": "^1.0.2",
    "@types/hpp": "^0.2.2",
    "@types/lodash": "^4.14.191",
    "@types/module-alias": "^2.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.15.3",
    "@types/qs": "^6.9.7",
    "@types/request-ip": "^0.0.37",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "firebase-functions-test": "^3.0.0",
    "prettier": "^2.8.4",
    "rimraf": "^4.4.0",
    "typescript": "^4.9.5"
  },
  "_moduleAliases": {
    "@core/*": "dist/@core/*",
    "@apps/*": "dist/apps/*",
    "@config/*": "dist/config/*",
    "@database/*": "dist/database/*",
    "@routes/*": "dist/routes/*"
  }
}
